{
  "name": "@acemarke/react-prod-sourcemaps",
  "version": "0.0.0",
  "description": "A tool to update app sourcemaps with the original code of ReactDOM's production builds",
  "repository": "https://github.com/markerikson/react-prod-sourcemaps",
  "author": "Mark Erikson",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/main.js",
  "bin": {
    "react-prod-sourcemaps": "lib/main.js"
  },
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "prepack": "yarn build",
    "start": "tsx ./src/main.ts"
  },
  "dependencies": {
    "@ampproject/remapping": "^2.2.1",
    "glob": "^10.3.3",
    "magic-string": "^0.30.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@types/yargs": "^17.0.24",
    "prettier": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  },
  "files": [
    "assets",
    "lib",
    "src"
  ]
}
